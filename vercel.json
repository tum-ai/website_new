{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "join.tum-ai.com"
        }
      ],
      "destination": "https://tum-ai.com/apply",
      "permanent": true
    }
  ],
  "rewrites": [{ "source": "/[^.]+", "destination": "/" }]
}
